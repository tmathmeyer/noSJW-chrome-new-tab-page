{
  "name": "NoSJW - New Tab Page",
  "description": "remove the shitty social justice links from the new tab page in chrome / chromium",
  "version": "1.0",
  "browser_action": {
    "default_title": "remove SJW"
  },
  "content_scripts": [
      {
        "matches": [ 
            "chrome://newtab/", 
            "https://www.google.com/_/chrome/newtab?espv=2&es_th=1&ie=UTF-8"
        ],
        "js": [ "sjw.js"]
      }
  ],
  "manifest_version": 2
}
